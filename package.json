{
  "name": "medium-clone",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": "14.17.0",
    "npm": "6.14.13"
  },
  "scripts": {
    "client": "cd client && yarn start",
    "server-dev": "set NODE_ENV=development&&nodemon server.js",
    "server-prod": "set NODE_ENV=production&&nodemon server.js",
    "server": "set NODE_ENV=development&&nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.26.0",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.11",
    "multer": "^1.4.2",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7"
  }
}
